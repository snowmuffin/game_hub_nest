# Space Engineers module-specific environment variables
# Copy to config/space-engineers/.env and adjust values

# Server connection
SE_SERVER_HOST=localhost
SE_SERVER_PORT=27016

# Item/Marketplace tuning (optional)
SE_DROP_RATE_MULTIPLIER=1.0
SE_MARKETPLACE_ENABLED=true

# Storage limits (optional)
SE_MAX_STORAGE_SLOTS=200
SE_MAX_ITEM_STACK=1000
